            <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:color-gray9\">{{ page.title }}</span>\r\n            {% elsif request.page_type contains 'product' %}\r\n              {% if collection.url %}\r\n                <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n                {{ collection.title | link_to: collection.url }}\r\n              {% endif %}\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:color-gray9\">{{ product.title }}</span>\r\n            {% elsif request.page_type contains 'collection' and collection.handle %}\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n              {% if current_tags %}\r\n                {% capture url %}/collections/{{ collection.handle }}{% endcapture %}\r\n                {{ collection.title | link_to: url }}\r\n                <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n                <span class=\"fz:15px fw:400 ff:font-primary c:color-gray9\">{{ current_tags | join: \" + \" }}</span>\r\n              {% else %}\r\n                <span class=\"fz:15px fw:400 ff:font-primary c:color-gray9\">{{ collection.title }}</span>\r\n              {% endif %}\r\n            {% elsif request.page_type == 'blog' %}\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n              {% if current_tags %}\r\n                {{ blog.title | link_to: blog.url }}\r\n                <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n                <span class=\"fz:15px fw:400 ff:font-primary c:color-gray9\">{{ current_tags | join: \" + \" }}</span>\r\n              {% else %}\r\n                <span class=\"fz:15px fw:400 ff:font-primary c:color-gray9\">{{ blog.title }}</span>\r\n              {% endif %}\r\n            {% elsif request.page_type == 'article' %}\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n              {{ blog.title | link_to: blog.url }}\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n              <span>{{ article.title }}</span>\r\n            {% elsif request.page_type == 'cart' %}\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:color-gray9\">Cart</span>\r\n            {% else %}\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:color-gray9\">{{ page_title }}</span>\r\n            {% endif %}\r\n        </nav>\r\n      {% endunless %}\r\n    </shopify>\r\n  </div>\r\n  <div class=\"container\">\r\n    <shopify>\r\n    <h3 class=\"mb:30px ta:center fz:35px fw:500 ff:font-primary fw:500\">{{'veda.customer.login_page.title' | t }}</h3>\r\n    <p class=\"mb:55px ta:center mt:-25px fz:25px ff:font-primary fw:400\">\r\n      {{'veda.customer.login_page.subtitle' | t }}\r\n    </p>\r\n      <div class=\"row jc:center\">\r\n        <div class=\"col-lg-5\">\r\n          <div class=\"pr:100px@md w:90% d:flex fld:column ai:flex-end@md\">\r\n            <div>\r\n              <h4 class=\"mb:25px fz:20px ff:font-primary fw:400\">{{'veda.customer.register.title' | t }}</h4>\r\n              <p class=\"mb:40px ff:font-primary fw:400 fz:15px c:color-gray9\">\r\n                {{'veda.customer.register.description' | t }}\r\n              </p>\r\n              <a href=\"{{routes.account_register_url}}\" class=\"js_add_ld button bgc:color-gray9 bgc:color-gray9|h bd:none! c:color-light c:color-light|h p:15px_0px_15px_0px p:15px_114px_15px_114px@md cur:pointer m:5px_5px_5px_5px fw:500 bdrs:0px! ff:font-primary fz:15px lts:0.15px w:100% m:0!\" id=\"customer_register_link\">{{'veda.customer.register.submit' | t }}</a>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"col-lg-5\">\r\n          <div class=\"w:100% w:500px@md d:flex fld:column\">\r\n            <h3 class=\"mb:20px fz:25px ff:font-primary fw:400\">{{'veda.customer.login_page.title' | t }}</h3>\r\n            {%- form 'customer_login', novalidate: 'novalidate' -%}\r\n              <p class=\"form-row\">\r\n                <input class=\"w:100%! w:500px!@md h:43px bd:1px_solid_color-gray3! pl:16px! c:color-gray3! bdrs:0px! fz:15px fw:400 fz:15px||placeholder fw:400||placeholder ff:font-primary!\" type=\"email\" name=\"customer[email]\" id=\"CustomerEmail\" autocomplete=\"email\" autocorrect=\"off\" autocapitalize=\"off\" {%- if form.errors contains 'form' %} class=\"input--error\" aria-invalid=\"true\"{%- endif -%} placeholder=\"{{'veda.customer.login_page.email' | t }}\"/>\r\n                <div class=\"mb:20px!\"></div>\r\n              </p>\r\n            \r\n              <p class=\"form-row\">\r\n                <input class=\"w:100%! w:500px!@md h:43px! mb:25px! bd:1px_solid_color-gray3! pl:16px! c:color-gray3 bdrs:0px! fz:15px fw:400 fz:15px||placeholder fw:400||placeholder ff:font-primary!\" type=\"password\" name=\"customer[password]\" id=\"CustomerPassword\"{%- if form.errors contains 'form' %} class=\"input--error\" aria-invalid=\"true\"{%- endif -%} placeholder=\"{{'veda.customer.login_page.password' | t }}\"/>\r\n              </p>\r\n            \r\n                <div class=\"mt:10px\"></div>\r\n                <a href=\"#recover\" data-no-instant rel=\"nofollow\" id=\"RecoverPassword\" class=\"td:underline c:color-gray9 fz:15px ff:font-primary fw:400 td:underline|h\" href=\"#\">\r\n                  {{'veda.customer.login_page.forgot_password' | t }}\r\n                </a>\r\n             \r\n              <div class=\"mb:40px\"></div>\r\n              <input type=\"submit\" class=\"btn js_add_ld ff:font-primary bgc:color-gray9 bgc:color-gray9|h bd:none! c:color-light c:color-light|h pt:15px pb:15px pl:pfs(20px,86px) pr:pfs(20px,86px) cur:pointer m:5px_5px_5px_5px fw:500 bdrs:0px! fz:15px lts:0.15px w:100%! m:0!\" value=\"{{'veda.customer.login_page.sign_in' | t }}\">\r\n            {%- endform -%}\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </shopify>\r\n  </div>\r\n</section>\r\n","schema":{"blocks":[{"id":"id_87db4bf7-8fe3-4187-a8e5-8d700d833346","children":[{"children":false,"type":"switch","label":"Background Fixed","summary":"","name":"background_fixed","id":"id_79252c94-f9bf-44e5-b230-9c4faa9371c3","disable":false},{"children":"none","type":"select","label":"Background Type","summary":"","name":"background_type","id":"id_3fa0a7df-079f-48b2-9963-bf3a6df35f6b","disable":false,"options":[{"label":"Image","value":"image","id":"d8ae11cc-981d-4cde-b9f9-63f829d24445"},{"label":"Color","value":"color","id":"8eb694b6-767b-41cb-8f1c-6f5228494ef0"},{"label":"None","value":"none","id":"3923427e-d71b-4dc1-9f64-5c8f2eabfb4b"}]},{"children":"","type":"image","label":"Background Image","summary":"","name":"background_image","id":"id_b5771642-446d-4ebd-86ee-f549bac63d94","disable":false,"deps":"section_settings.background_type === \"image\""},{"children":"#fff","type":"color","label":"Background Color","summary":"","name":"background_color","id":"id_f83ac643-cf3b-4c97-8a5f-0c2cf3be67dc","disable":false,"deps":"section_settings.background_type === \"color\""},{"children":"cover","type":"select","label":"Background Size","summary":"","name":"background_size","id":"id_8e172929-3630-4e49-9b0e-76e75c5aa4f3","disable":false,"options":[{"label":"Cover","value":"cover","id":"3caac4c5-3c65-4ddd-a3bb-425a66669ce1"},{"label":"Contain","value":"contain","id":"9c035798-6f6a-4d31-8661-6caa6f0f4530"}