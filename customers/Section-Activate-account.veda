{"id":"id_cb626ff4-9661-486b-b656-9dfc6d596f8f","commandId":"62c7d19c6bec6e2bff841571","label":"Activate account","image":{"src":"","width":0,"height":0},"addonIds":[],"megaMenuId":"","currentVersion":"1","versionId":"","data":{"js":"","liquid":"{% liquid\r\n  if section_settings.background_fixed assign backgroundFixed = 'fixed' else assign backgroundFixed = 'scroll' endif\r\n  if section_settings.background_type == 'color' assign backgroundColor = 'background-color: ' | append: section_settings.background_color | append: ';' else assign backgroundColor = '' endif\r\n  if section_settings.background_type == 'image' assign backgroundImage = 'background-image: url(' | append: section_settings.background_image | append:\r\n  ');background-attachment: ' | append: backgroundFixed | append: ';background-size: ' | append: section_settings.background_size | append: ';' else assign backgroundImage = '' endif\r\n  assign sectionPadding = 'padding-top: ' | append: section_settings.padding.top | append: 'px;padding-right: ' | append: section_settings.padding.right | append: 'px;padding-bottom: ' | append: section_settings.padding.bottom | append: 'px;padding-left: ' | append: section_settings.padding.left | append: 'px;'\r\n%}\r\n\r\n\r\n<section data-id=\"{{uniqueId}}\" css=\"{{ backgroundColor }}{{ backgroundImage }}{{ sectionPadding }}\">\r\n  {% if section_settings.background_overlay_enable %}\r\n    <div class=\"pos:absolute ins:0 z:-1\" css=\"background-color: {{ section_settings.background_overlay_color }}\"></div>\r\n  {% endif %}\r\n  <div class=\"fz:15px c:color-gray4 mb:33px ml:5%\">\r\n    <shopify>\r\n      {% unless request.page_type == 'index' or request.page_type == 'list-collections' %}\r\n        <nav role=\"navigation\" aria-label=\"breadcrumbs\">\r\n            <a class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" href=\"{{ routes.root_url }}\" title=\"Home\">Home</a>\r\n            {% if request.page_type contains 'page' %}\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\"></span>\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:color-gray9\">{{ page.title }}</span>\r\n            {% elsif request.page_type contains 'product' %}\r\n              {% if collection.url %}\r\n                <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\"></span>\r\n                {{ collection.title | link_to: collection.url }}\r\n              {% endif %}\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\"></span>\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:color-gray9\">{{ product.title }}</span>\r\n            {% elsif request.page_type contains 'collection' and collection.handle %}\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\"></span>\r\n              {% if current_tags %}\r\n                {% capture url %}/collections/{{ collection.handle }}{% endcapture %}\r\n                {{ collection.title | link_to: url }}\r\n                <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\"></span>\r\n                <span class=\"fz:15px fw:400 ff:font-primary c:color-gray9\">{{ current_tags | join: \" + \" }}</span>\r\n              {% else %}\r\n                <span class=\"fz:15px fw:400 ff:font-primary c:color-gray9\">{{ collection.title }}</span>\r\n              {% endif %}\r\n            {% elsif request.page_type == 'blog' %}\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\"></span>\r\n              {% if current_tags %}\r\n                {{ blog.title | link_to: blog.url }}\r\n                <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\"></span>\r\n                <span class=\"fz:15px fw:400 ff:font-primary c:color-gray9\">{{ current_tags | join: \" + \" }}</span>\r\n              {% else %}\r\n                <span class=\"fz:15px fw:400 ff:font-primary c:color-gray9\">{{ blog.title }}</span>\r\n              {% endif %}\r\n            {% elsif request.page_type == 'article' %}\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\"></span>\r\n              {{ blog.title | link_to: blog.url }}\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\"></span>\r\n              <span>{{ article.title }}</span>\r\n            {% elsif request.page_type == 'cart' %}\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\"></span>\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:color-gray9\">Cart</span>\r\n            {% else %}\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\"></span>\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:color-gray9\">{{ page_title }}</span>\r\n            {% endif %}\r\n        </nav>\r\n      {% endunless %}\r\n    </shopify>\r\n  </div>\r\n  <div class=\"container\">\r\n    <shopify>\r\n    <h3 class=\"mb:30px ta:center fz:35px fw:500 ff:font-primary fw:500\">{{'veda.customer.activate_account.title' | t }}</h3>\r\n    <p class=\"mb:55px ta:center mt:-25px fz:15px ff:font-primary fw:400\">\r\n      {{'veda.customer.activate_account.subtext' | t }}\r\n    </p>\r\n      <div class=\"row jc:center\">\r\n        <div class=\"col-lg-5\">\r\n          <div class=\"\">\r\n            {% form 'activate_customer_password' %}\r\n    \r\n              <div class=\"password\">\r\n                <input type=\"password\" class=\"bdrs:0px! w:100%! maw:500px h:43px p:7px_15px bdc:var(--color-gray3) bds:solid bdw:1px\" name=\"customer[password]\" placeholder=\"{{'veda.customer.activate_account.password' | t }}\" />\r\n              </div>\r\n              <div class=\"h:20px\"></div>\r\n              <div class=\"password_confirm\">\r\n                <input type=\"password\" class=\"bdrs:0px! w:100%! maw:500px h:43px p:7px_15px bdc:var(--color-gray3) bds:solid bdw:1px\" name=\"customer[password_confirmation]\" placeholder=\"{{'veda.customer.activate_account.password_confirm' | t }}\"/>\r\n              </div>\r\n              <div class=\"h:20px\"></div>\r\n              <div class=\"submit d:flex jc:center\">\r\n                <input type=\"submit\" class=\"bgc:color-gray9 bgc:color-gray9|h! bd:1px_solid_color-gray9 bd:1px_solid_color-gray9|h! p:10px_20px_10px_20px c:color-light c:color-light|h!\" value=\"{{'veda.customer.activate_account.submit' | t }}\" />\r\n                <div class=\"w:20px\"></div>\r\n                <input type=\"submit\" class=\"bgc:color-gray9 bgc:color-gray9|h! bd:1px_solid_color-gray9 bd:1px_solid_color-gray9|h! p:10px_20px_10px_20px c:color-light c:color-light|h!\" name=\"decline\" value=\"{{'veda.customer.activate_account.cancel' | t }}\" />\r\n              </div>\r\n            {% endform %}\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </shopify>\r\n  </div>\r\n</section>\r\n","schema":{"blocks":[{"id":"id_8f8062a8-366d-4cb7-9083-fc2c8c5b56fd","children":[{"children":false,"type":"switch","label":"Background Fixed","summary":"","name":"background_fixed","id":"id_f796d863-4fb2-4f30-bfda-d6613892e205","disable":false},{"children":"none","type":"select","label":"Background Type","summary":"","name":"background_type","id":"id_4b313d60-8eda-427d-9c4f-5d45ea34a0cc","disable":false,"options":[{"label":"Image","value":"image","id":"1c577702-1fde-4d6e-813d-13b20431516d"},{"label":"Color","value":"color","id":"1cf0a24f-df58-498f-9de3-b4e041b5e3ab"},{"label":"None","value":"none","id":"2e7746e7-57a5-44fd-a4f1-ce6a0a84cbd2"}]},{"children":"","type":"image","label":"Background Image","summary":"","name":"background_image","id":"id_36b17875-a9a8-4cd6-a9f7-877e5ececd39","disable":false,"deps":"section_settings.background_type === \"image\""},{"children":"#fff","type":"color","label":"Background Color","summary":""