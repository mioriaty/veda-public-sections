{"id":"id_1e39215e-ef71-494e-85c6-779cbe63b5a7","commandId":"62a93ed6d316833cf19c25b4","label":"Product Detail","image":{"src":"","width":0,"height":0},"addonIds":[],"megaMenuId":"","currentVersion":"1.2","versionId":"d9d18f6f-02fd-4df8-b646-e15bdaad646a","data":{"js":"\r\nlet loadding = false;\r\nconst { cart, productWishList, imageZoom } = veda.plugins;\r\nconst { objectParse } = veda.utils;\r\nclass PageProduct {\r\n  constructor() {\r\n    this.init();\r\n    productWishList.subscribe(this.handleChangeStatus.bind(this));\r\n  }\r\n  handleChangeImage(event) {\r\n    const imgEl = container.querySelector(\".yasmina-page-product__img\");\r\n    const currentData = event.currentTarget;\r\n    const data = event.currentTarget.parentNode.querySelector(\".page-product-variant-data-js\").textContent;\r\n    container.querySelector(\".page-product-color-js\").innerHTML = `Color : ${JSON.parse(data).image.color}`;\r\n    imgEl.src = currentData.src;\r\n    const images = container.querySelectorAll(\".page-product-image-select-js\");\r\n    images.forEach(image => {\r\n      image.classList.remove(\"bd:1px_solid_color-dark\");\r\n    })\r\n    currentData.classList.add(\"bd:1px_solid_color-dark\");\r\n  }\r\n  handleChangeStatus() {\r\n    const data = productWishList.getData();\r\n    const productEl = container.querySelector(\".yasmina-page-product\");\r\n    const btnWishlist = productEl.querySelector(\".wishlist-toggle-js\");\r\n    const dataEl = productEl.querySelector(\".yasmina-page-product__data\");\r\n    const newItem = objectParse(dataEl.textContent);\r\n    const { id: newId } = newItem;\r\n    const dataHasNewItem = data.some(item => item.id === newId);\r\n    if (dataHasNewItem) {\r\n      btnWishlist.style.backgroundColor = \"#AF0707\";\r\n      btnWishlist.style.color = \"#ffffff\";\r\n    } else {\r\n      btnWishlist.style.backgroundColor = \"#ffffff\";\r\n      btnWishlist.style.color = \"#000000\";\r\n    }\r\n  }\r\n  handleDOM() {\r\n    this.handleChangeStatus();\r\n    const productEl = container.querySelector(\".yasmina-page-product\");\r\n    const dataEl = productEl.querySelector(\".yasmina-page-product__data\");\r\n    const dataWishList = objectParse(dataEl.textContent);\r\n    const sizeElText = container.querySelector(\".yasmina-page-product__sizes\");\r\n    const sizeElList = container.querySelectorAll(\".yasmina-page-product__size-input\");\r\n    sizeElList.forEach(sizeEl => {\r\n      sizeEl.addEventListener(\"click\", () => {\r\n        sizeElText.textContent = \"Size : \" + sizeEl.value;\r\n      });\r\n    })\r\n    //button add cart\r\n    const btnAddCart = document.querySelector('.product-card-add-js');\r\n    btnAddCart.addEventListener('click', () => {\r\n      if (!loadding) {\r\n        const quantityEl = document.querySelector('.yasmina-page-product__quantity');\r\n        const itemAdd = cart.getData().filter(item => item.product_id === dataWishList.id);\r\n        loadding = true;\r\n        const spinner = \"<div class='veda-spinner' style='--spinner-color:red'></div>\";\r\n        btnAddCart.insertAdjacentHTML('afterbegin', spinner);\r\n        if (itemAdd.length > 0) {\r\n          cart.updateCart(itemAdd[0].id, Number(itemAdd[0].quantity) + Number(quantityEl.value)).finally(() => {\r\n            const currentSpinner = btnAddCart.querySelector('.veda-spinner');\r\n            currentSpinner.remove();\r\n            loadding = false;\r\n          });\r\n        } else {\r\n          cart.addToCart(dataWishList).finally(() => {\r\n            const currentSpinner = btnAddCart.querySelector('.veda-spinner');\r\n            currentSpinner.remove();\r\n            loadding = false;\r\n          });\r\n        }\r\n      }\r\n    });\r\n    const btnWishlist = container.querySelector(\".wishlist-toggle-js\");\r\n    btnWishlist.addEventListener(\"click\", () => {\r\n      productWishList.toggleWishList(dataWishList);\r\n    })\r\n  }\r\n  init() {\r\n    const images = container.querySelectorAll(\".page-product-image-select-js\");\r\n    images.forEach(image => {\r\n      image.addEventListener(\"click\", this.handleChangeImage.bind(this));\r\n    })\r\n    this.handleDOM();\r\n  }\r\n}\r\nnew PageProduct();\r\nimageZoom(container);\r\n","liquid":"{% liquid\r\nif section_settings.background_fixed assign backgroundFixed = 'fixed' else assign backgroundFixed = 'scroll' endif\r\nif section_settings.background_type == 'color' assign backgroundColor = 'background-color: ' | append:\r\nsection_settings.background_color | append: ';' else assign backgroundColor = '' endif\r\nif section_settings.background_type == 'image' assign backgroundImage = 'background-image: url(' | append:\r\nsection_settings.background_image | append:\r\n');background-attachment: ' | append: backgroundFixed | append: ';background-size: ' | append:\r\nsection_settings.background_size | append: ';' else assign backgroundImage = '' endif\r\nassign sectionPadding = 'padding-top: ' | append: section_settings.padding.top | append: 'px;padding-right: ' | append:\r\nsection_settings.padding.right | append: 'px;padding-bottom: ' | append: section_settings.padding.bottom | append:\r\n'px;padding-left: ' | append: section_settings.padding.left | append: 'px;'\r\n%}\r\n\r\n\r\n<section data-id=\"{{uniqueId}}\" css=\"{{ backgroundColor }}{{ backgroundImage }}{{ sectionPadding }} product-detail-yasmina\">\r\n  {% if section_settings.background_overlay_enable %}\r\n  <div class=\"pos:absolute ins:0 z:-1\" css=\"background-color: {{ section_settings.background_overlay_color }}\"></div>\r\n  {% endif %}\r\n  <div class=\"container pt:35px\">\r\n    <div class=\"fz:15px c:color-gray4 mb:33px ff:font-primary fw:400\">\r\n      <shopify>\r\n        {% unless request.page_type == 'index' or request.page_type == 'list-collections' %}\r\n        <nav role=\"navigation\" aria-label=\"breadcrumbs\">\r\n          <a class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" href=\"{{ routes.root_url }}\" title=\"Home\">Home</a>\r\n          {% if request.page_type contains 'page' %}\r\n          <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n          <span class=\"fz:15px fw:400 ff:font-primary c:color-gray9\">{{ page.title }}</span>\r\n          {% elsif request.page_type contains 'product' %}\r\n          {% if collection.url %}\r\n          <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n          {{ collection.title | link_to: collection.url }}\r\n          {% endif %}\r\n          <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n          <span class=\"fz:15px fw:400 ff:font-primary c:color-gray9\">{{ product.title }}</span>\r\n          {% elsif request.page_type contains 'collection' and collection.handle %}\r\n          <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n          {% if current_tags %}\r\n          {% capture url %}/collections/{{ collection.handle }}{% endcapture %}\r\n          {{ collection.title | link_to: url }}\r\n          <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n          <span class=\"fz:15px fw:400 ff:font-primary c:color-gray9\">{{ current_tags | join: \" + \" }}</span>\r\n          {% else %}\r\n          <span class=\"fz:15px fw:400 ff:font-primary c:color-gray9\">{{ collection.title }}</span>\r\n          {% endif %}\r\n          {% elsif request.page_type == 'blog' %}\r\n          <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n          {% if current_tags %}\r\n          {{ blog.title | link_to: blog.url }}\r\n          <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n          <span class=\"fz:15px fw:400 ff:font-primary c:color-gray9\">{{ current_tags | join: \" + \" }}</span>\r\n          {% else %}\r\n          <span class=\"fz:15px fw:400 ff:font-primary c:color-gray9\">{{ blog.title }}</span>\r\n          {% endif %}\r\n          {% elsif request.page_type == 'article' %}\r\n          <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n          {{ blog.title | link_to: blog.url }}\r\n          <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n          <span>{{ article.title }}</span>\r\n          {% elsif request.page_type == 'cart' %}\r\n          <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n          <span class=\"fz:15px fw:400 ff:font-primary c:color-gray9\">Cart</span>\r\n          {% else %}\r\n          <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n          <span class=\"fz:15px fw:400 ff:font-primary c:color-gray9\">{{ page_title }}</span>\r\n          {% endif %}\r\n        </nav>\r\n        {% endunless %}\r\n      </shopify>\r\n    </div>\r\n    <div class=\"row\">\r\n      <div\r\n        class=\"row row-cols-lg-{{ content.column.lg }} row-cols-md-{{ content.column.md }} row-cols-sm-{{ content.column.sm }} row-cols-{{ content.column.xs }}\">\r\n        <div class=\"col d:flex fld:column ai:center pr:30px\">\r\n          <shopify>\r\n            <a href=\"#\" class=\"veda-image-zoom veda-image-cover yasmina-page-product__image d:block w:100%\"\r\n              css=\"--aspect-ratio: 3/4\" data-image-zoom-src=\"{{ image.src }}\" data-image-zoom=\"2\">\r\n              <img class=\"w:100% veda-image-cover yasmina-page-product__img\"\r\n                src=\"{{ product.featured_image.src | img_url: '900x' }}\" alt=\"\">\r\n            </a>\r\n          </shopify>\r\n          <button\r\n            class=\"w:90% w:490px@sm h:60px lh:44px bgc:transparent c:color-gray9 bd:1px_solid_color-gray4 ai:center jc:center mt:35px fw:500 fz:15px maw:334px ta:center bdrs:0px! bgc:transparent|h! c:color-gray9|h! bd:1px_solid_color-gray4|h! cur:pointer\">\r\n            <i\r\n              class=\"far fa-question mr:10px fw:400 fz:15px bgc:transparent|h! c:color-gray9|h! bd:1px_solid_color-gray4|h!\">\r\n            </i>{{content.button_ask_expert_text}}</button>\r\n        </div>\r\n        <div class=\"col\">\r\n          <shopify>\r\n            <div class=\"yasmina-page-product\">\r\n              <script class=\"yasmina-page-product__data\" type=\"application/json\">\r\n              {\r\n                id: {{ product.id | json }},\r\n                title: {{ product.title | json }},\r\n                vendor: {{ product.vendor | json }},\r\n                handle: {{ product.handle | json }},\r\n                available: {{ product.available | json }},\r\n                content: {{ product.content | json }},\r\n                description: {{ product.description | json }},\r\n                featured_image: {{ product.featured_image | json }},\r\n                images: {{ product.images | json }},\r\n                options: {{ product.options | json }},\r\n                price_max: {{ product.price_max | json }},\r\n                price_min: {{ product.price_min | json }},\r\n                type: {{ product.type | json }},\r\n                tags: {{ product.tags | json }},\r\n                variants: {{ product.variants | json }},\r\n                price: {{ product.price | json }},\r\n                options_with_values: {{ product.options_with_values | json }},\r\n                url: {{ product.url | json }}\r\n\r\n              }\r\n            </script>\r\n              <h3 class=\"fz:25px ff:font-primary c:color-dark fw:500\">{{product.title}}</h3>\r\n              <div class=\"d:flex ai:center mt:15px mb:16px c:color-dark fz:15px fw:300\">\r\n                {% if content.review_app_enable %}\r\n                {% case content.review_app_type %}\r\n                  {% when 'product_reviews' %}\r\n                    <rivio-review-badge item=\"product\" />\r\n                    <span class=\"shopify-product-reviews-badge\" data-id=\"{{ product.id }}\"></span>\r\n                  {% when 'rivyo_reviews' %}\r\n                    <rivio-review-badge item=\"product\" />\r\n                  {% when 'loox_reviews' %}\r\n                    <loox-review-badge item=\"product\" />\r\n                  {% when 'ali_reviews' %}\r\n                    <ali-review-badge item=\"product\" />\r\n                  {% when 'stamped_reviews' %}\r\n                    <stamped-review-badge item=\"product\" />\r\n                  {% when 'judge_me_reviews' %}\r\n                    <judge-me-review-badge item=\"product\" />\r\n                  {% when 'other' %}\r\n                    {{ content.review_app_html }}\r\n                {% endcase %}\r\n              {% endif %}\r\n              </div>\r\n              <p class=\"mb:16px ff:font-primary c:color-dark fz:15px fw:300\">Vendor: {{product.vendor}}</p>\r\n              <p class=\"mb:16px ff:font-primary c:color-dark fz:15px fw:300\">SKU: {{product.variants[0].sku}}</p>\r\n              <p class=\"mb:16px ff:font-primary c:color-dark fz:15px fw:300\">Availability: {{product.variants.length}}\r\n                in stock</p>\r\n              <p class=\"mb:16px ff:font-primary c:color-dark fz:15px fw:300\">Product Type: {{product.type}}</p>\r\n              <h3 class=\"mb:25px fz:25px ff:font-primary c:color-dark fw:500\">${{product.price}}.00</h3>\r\n              <div class=\"lines-3 ff:font-secondary c:color-dark fz:13px fw:300\">{{product.description | strip_html}}\r\n              </div>\r\n              <p class=\"page-product-color-js mb:7px mt:29px ff:font-primary c:color-dark fz:15px fw:300\">Color :\r\n                {{product.options_with_values[1].selected_value}}</p>\r\n              <div class=\"d:flex mb:45px\">\r\n                {% for item in product.variants %}\r\n                {% for itemoptioncolor in product.options_with_values %}\r\n                {% if itemoptioncolor.name == \"Color\" %}\r\n                {% if item.options[1] == itemoptioncolor.selected_value %}\r\n                <div class=\"veda-image-cover w:90px mr:10px cur:pointer\" css=\"--aspect-ratio: 3/4\">\r\n\r\n                  <script class=\"page-product-variant-data-js\" type=\"application/json\">{{ item | json }}</script>\r\n                  <img class=\"bd:1px_solid_color-dark page-product-image-select-js\"\r\n                    src=\"{{ item.image.src | img_url: '900x' }}\" alt=\"\">\r\n                </div>\r\n                {% else %}\r\n                <div class=\"veda-image-cover w:90px mr:10px cur:pointer\" css=\"--aspect-ratio: 3/4\">\r\n                  <script class=\"page-product-variant-data-js\" type=\"application/json\">{{ item | json }}</script>\r\n                  <img class=\"page-product-image-select-js\" src=\"{{ item.image.src | img_url: '660x' }}\" alt=\"\">\r\n                </div>\r\n                {% endif %}\r\n                {% endif %}\r\n                {% endfor %}\r\n\r\n                {% endfor %}\r\n\r\n\r\n              </div>\r\n              <p class=\"yasmina-page-product__sizes mb:7px ff:font-primary c:color-dark fz:15px fw:300\">Size :\r\n                {{product.options_with_values[0].selected_value}}</p>\r\n              <div class=\"mb:30px d:flex fxw:wrap\">\r\n                {% for itemoption in product.options_with_values %}\r\n                  {% if itemoption.name == \"Size\" %}\r\n                {% for item in itemoption.values %}\r\n                <label\r\n                  class=\"veda-radio d:flex pos:relative ai:center jc:center w:70px h:34px mr:10px ta:center cur:pointer ff:font-primary fz:15px mt:10px mt:0px@sm \"\r\n                  style=\"--size: 15px;--color: var(--color-gray9)\">\r\n                  <div class=\"veda-radio__inner\">\r\n                    <input type=\"radio\" data-label=\"{{ item }}\" name=\"yasmina-product-page-radio-size\"\r\n                      value=\"{{ item }}\" class=\"yasmina-page-product__size-input\" {% if item==itemoption.selected_value\r\n                      %}checked{% endif %} />\r\n                    <div class=\"veda-radio__track w:70px h:34px bdrs:0px bgc:color-gray2 bd:1px_solid_color-gray2 op:1\">\r\n                      {{ item }}\r\n                    </div>\r\n                  </div>\r\n                </label>\r\n\r\n                {% endfor %}\r\n                {% endif %}\r\n                {% endfor %}\r\n              </div>\r\n\r\n              <div class=\"d:flex ai:center fxw:wrap\">\r\n                <input\r\n                  class=\"yasmina-page-product__quantity w:85px! h:50px bdrs:0px! ta:center fw:300 fz:15px bd:1px_solid_color-gray4\"\r\n                  type=\"number\" value=\"1\" min=\"1\">\r\n                <div class=\"w:10px\"></div>\r\n                <button\r\n                  class=\"product-card-add-js h:50px d:flex ai:center jc:center mah:50px mr:10px ff:font-primary bgc:color-dark bd:none! c:color-light cur:pointer fw:500 bdrs:0px! fz:15px lts:0.15px fw:500 bdrs:0px! bgc:color-dark|h! c:color-light|h! w:120px w:236px@sm ta:center p:0px mt:10px mt:10px@sm mb:10px mb:10px@sm tt:uppercase\">{{content.button_add_to_cart_text}}</button>\r\n                <div\r\n                  class=\"wishlist-toggle-js w:50px h:50px bd:1px_solid_color-gray4 d:flex ai:center jc:center mr:10px cur:pointer bgc:color-dark|h c:red|h mt:10px mt:10px@sm mb:10px mb:10px@sm\">\r\n                  <i class=\"far fa-heart fz:15px fw:400\"></i>\r\n                </div>\r\n                <div\r\n                  class=\"w:50px h:50px bd:1px_solid_color-gray4 d:flex ai:center jc:center cur:pointer bgc:color-dark|h c:red|h mt:10px mt:10px@sm mb:10px mb:10px@sm\">\r\n                  <i class=\"fas fa-share-alt fz:15px fw:400\"></i>\r\n                </div>\r\n              </div>\r\n              <div class=\"d:flex ai:center mt:20px\">\r\n                <input class=\"mr:10px\" type=\"ch