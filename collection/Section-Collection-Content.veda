{"id":"id_c124118a-a67f-45a4-9a01-bc5e7d403124","commandId":"62a9abb6d316833cf19c25c0","label":"Collection Content","image":{"src":"","width":0,"height":0},"addonIds":[],"megaMenuId":"","currentVersion":"1.1","versionId":"880f87d1-97d7-41bf-a7ad-27624e44202a","data":{"js":"const {\r\n  collectionsFilters,\r\n  message,\r\n  productCompare,\r\n  productWishList,\r\n  productQuickView,\r\n  productColor,\r\n  cart,\r\n} = veda.plugins;\r\nconst { objectParse, queryString } = veda.utils;\r\nconst { html } = veda.utils.csr;\r\nlet unsubscribeCompare = () => { };\r\nlet unsubscribeWishList = () => { };\r\nfunction checkHasItem(productData, storeData) {\r\n  return storeData?.some((item) => item.id === productData.id);\r\n}\r\nfunction changeStatus(el, hasItem) {\r\n  if (hasItem()) {\r\n    el.setAttribute(\"data-tooltip-active\", true);\r\n    el.classList.remove(\"bgc:color-light\", \"c:color-primary\");\r\n    el.classList.add(\"bgc:color-primary\", \"c:color-light-freeze\");\r\n  } else {\r\n    el.setAttribute(\"data-tooltip-active\", false);\r\n    el.classList.remove(\"bgc:color-primary\", \"c:color-light-freeze\");\r\n    el.classList.add(\"bgc:color-light\", \"c:color-primary\");\r\n  }\r\n  return hasItem;\r\n}\r\nfunction handleCompare() {\r\n  const listCard = container.querySelectorAll(\".product-card-js\");\r\n  listCard.forEach((card) => {\r\n    const compareDataEl = card.querySelector(\".product-card-data-js\");\r\n    const productData = objectParse(compareDataEl.textContent);\r\n    const btnCompareEl = card.querySelector(\".compare-toggle-js\");\r\n    const ratingCustom = card.querySelector(\".compare-rating-js\");\r\n    if (!btnCompareEl) {\r\n      return;\r\n    }\r\n    const hasItem = () => checkHasItem(productData, productCompare.getData());\r\n    changeStatus(btnCompareEl, hasItem);\r\n    btnCompareEl.addEventListener(\"click\", () => {\r\n      productCompare.toggleProduct({\r\n        ...productData,\r\n        rating: ratingCustom?.innerHTML,\r\n      });\r\n      const tooltipText = btnCompareEl.getAttribute(\"data-tooltip-text\");\r\n      const tooltipActiveText = btnCompareEl.getAttribute(\r\n        \"data-tooltip-active-text\"\r\n      );\r\n      changeStatus(btnCompareEl, hasItem);\r\n      if (hasItem()) {\r\n        tooltipText && message.success(tooltipText);\r\n      } else {\r\n        tooltipActiveText && message.error(tooltipActiveText);\r\n      }\r\n    });\r\n  });\r\n  unsubscribeCompare();\r\n  unsubscribeCompare = productCompare.subscribe((state) => {\r\n    const listCard = container.querySelectorAll(\".product-card-js\");\r\n    listCard.forEach((card) => {\r\n      const compareDataEl = card.querySelector(\".product-card-data-js\");\r\n      const productData = objectParse(compareDataEl.textContent);\r\n      const btnCompareEl = card.querySelector(\".compare-toggle-js\");\r\n      const hasItem = () => checkHasItem(productData, state);\r\n      changeStatus(btnCompareEl, hasItem);\r\n    });\r\n  });\r\n}\r\nfunction handleWishList() {\r\n  const listCard = container.querySelectorAll(\".product-card-js\");\r\n  listCard.forEach((card) => {\r\n    const compareDataEl = card.querySelector(\".product-card-data-js\");\r\n    const productData = objectParse(compareDataEl.textContent);\r\n    const btnWishListEl = card.querySelector(\".wishlist-toggle-js\");\r\n    if (!btnWishListEl) {\r\n      return;\r\n    }\r\n    const hasItem = () =>\r\n      checkHasItem(productData, productWishList.getData());\r\n    productQuickView.customQuickView({\r\n      link: productData.url,\r\n    });\r\n    changeStatus(btnWishListEl, hasItem);\r\n    btnWishListEl.addEventListener(\"click\", () => {\r\n      productWishList.toggleWishList(productData);\r\n      const tooltipText = btnWishListEl.getAttribute(\"data-tooltip-text\");\r\n      const tooltipActiveText = btnWishListEl.getAttribute(\r\n        \"data-tooltip-active-text\"\r\n      );\r\n      changeStatus(btnWishListEl, hasItem);\r\n      if (hasItem()) {\r\n        tooltipText && message.success(tooltipText);\r\n      } else {\r\n        tooltipActiveText && message.error(tooltipActiveText);\r\n      }\r\n    });\r\n  });\r\n  unsubscribeWishList();\r\n  unsubscribeWishList = productWishList.subscribe((state) => {\r\n    const listCard = container.querySelectorAll(\".product-card-js\");\r\n    listCard.forEach((card) => {\r\n      const dataEl = card.querySelector(\".product-card-data-js\");\r\n      const productData = objectParse(dataEl.textContent);\r\n      const btnWishList = card.querySelector(\".wishlist-toggle-js\");\r\n      const hasItem = () => checkHasItem(productData, state);\r\n      changeStatus(btnWishList, hasItem);\r\n    });\r\n  });\r\n}\r\nfunction handleCart() {\r\n  if (builderMode) {\r\n    return;\r\n  }\r\n  const listCard = container.querySelectorAll(\".product-card-js\");\r\n  listCard.forEach((card) => {\r\n    const cartDataEl = card.querySelector(\".product-card-data-js\");\r\n    const productData = objectParse(cartDataEl.textContent);\r\n    const btnAddCart = card.querySelector(\".product-card-add-js\");\r\n    if (!btnAddCart) {\r\n      return;\r\n    }\r\n    btnAddCart.addEventListener(\"click\", async (event) => {\r\n      event.preventDefault();\r\n      await cart.addToCart(productData);\r\n      cart.toggleDraw();\r\n    });\r\n  });\r\n}\r\nfunction handleQuickView() {\r\n  const listCard = container.querySelectorAll(\".product-card-js\");\r\n  listCard.forEach((card) => {\r\n    const cartDataEl = card.querySelector(\".product-card-data-js\");\r\n    const productData = objectParse(cartDataEl.textContent);\r\n    const btnQuickView = card.querySelector(\".quickview-toggle-js\");\r\n    if (!btnQuickView) {\r\n      return;\r\n    }\r\n    btnQuickView.addEventListener(\"click\", () => {\r\n      productQuickView.togglePopup(productData);\r\n    });\r\n  });\r\n}\r\nfunction handleColor() {\r\n  const listCard = container.querySelectorAll(\".product-card-js\");\r\n  productColor.init({\r\n    onChange: (color, image, currentEl) => {\r\n      const currentImage = currentEl\r\n        .closest(\".product-card-js\")\r\n        .querySelector(\".product-card-image-js\");\r\n      currentImage.src = image;\r\n    },\r\n  });\r\n  listCard.forEach((card) => {\r\n    const cartDataEl = card.querySelector(\".product-card-data-js\");\r\n    const productData = objectParse(cartDataEl.textContent);\r\n    const colorWrapper = card.querySelector(\".product-card-colors-js\");\r\n    if (!colorWrapper) {\r\n      return;\r\n    }\r\n    productColor.render(colorWrapper, productData);\r\n  });\r\n}\r\nfunction productActions() {\r\n  handleCompare();\r\n  handleWishList();\r\n  handleQuickView();\r\n  handleCart();\r\n  handleColor();\r\n}\r\nproductActions();\r\n\r\n// FILTER\r\nasync function getContentCollections(url) {\r\n  const domParser = new DOMParser();\r\n  const res = await fetch(url);\r\n  const html = await res.text();\r\n  const doc = domParser.parseFromString(html, \"text/html\");\r\n  const contentCollectionsEl = doc.querySelector(\".content-collections-js\");\r\n  if (contentCollectionsEl) {\r\n    const contentCollections = contentCollectionsEl.innerHTML;\r\n    return contentCollections;\r\n  }\r\n  return \"\";\r\n}\r\n\r\ncollectionsFilters(container, {\r\n  formSelector: \".filter-form-js\",\r\n  sortBySelector: \".filter-sort-by-js\",\r\n  refineRootSelector: \".filter-refine-js\",\r\n  clearAllRootSelector: \".filter-clear-all-js\",\r\n  priceStep: 0.01,\r\n  renderRefineItem({ item, onRemove }) {\r\n    return html`\r\n      <div\r\n      key=${item.value}\r\n      class=\"pos:relative d:inline-block bd:1px_solid_color-gray2 w:fit-content p:2px_25px_2px_5px cur:pointer c:color-gray4 ff:font-primary mr:5px mb:5px\"\r\n    >\r\n      <span class=\"c:color-gray4\">${item.label}</span>\r\n      <span\r\n        class=\"cur:pointer\"\r\n      >\r\n        <i class=\"fal fa-times pos:absolute t:55% r:1% trf:translate(-50%,-50%) c:color-gray4 c:color-gray9!|h\" onClick=${onRemove}></i>\r\n      </span>\r\n    </div>\r\n      `;\r\n  },\r\n  renderClearAllButton({ onClear }) {\r\n    const text =\r\n      container\r\n        .querySelector(\".filter-clear-all-js\")\r\n        ?.getAttribute(\"data-text\") ?? \"Clear All\";\r\n    return html`<div\r\n        class=\"c:color-gray9 ff:font-primary bgc:color-light bgc:color-light|h! bd:none! c:color-gray9|h! p:0! c:color-primary|h!\"\r\n        onClick=${onClear}\r\n      >\r\n        ${text}\r\n      </div>`;\r\n  },\r\n  async onChange({ url, urlRequest, category, done }) {\r\n    if (builderMode) {\r\n      return;\r\n    }\r\n    try {\r\n      const sectionId = container\r\n        .querySelector(\"[data-shopify-id]\")\r\n        ?.getAttribute(\"data-shopify-id\");\r\n      if (category) {\r\n        url.pathname = `/${category}`;\r\n        urlRequest.pathname = `/${category}`;\r\n      }\r\n      if (sectionId) {\r\n        urlRequest.search = `${urlRequest.search}&section_id=${sectionId}`;\r\n      }\r\n      const contentCollectionsEl = container.querySelector(\r\n        \".content-collections-js\"\r\n      );\r\n      contentCollectionsEl.style.opacity = 0.6;\r\n      const content = await getContentCollections(urlRequest);\r\n      contentCollectionsEl.style.opacity = 1;\r\n      contentCollectionsEl.innerHTML = content;\r\n      productActions();\r\n      done();\r\n    } catch (err) {\r\n      console.log(err);\r\n    }\r\n  },\r\n  onChangePrice({ min, max }) {\r\n    const priceViewEl = container.querySelector(\".filter-price-view-js\");\r\n    priceViewEl.textContent = `${min} - ${max}`;\r\n  },\r\n  refineListener(items) {\r\n    const refineWrapperEl = container.querySelector(\r\n      \".filter-refine-wrapper-js\"\r\n    );\r\n    if (items.length) {\r\n      refineWrapperEl.classList.add(\"d:block\");\r\n    } else {\r\n      refineWrapperEl.classList.remove(\"d:block\");\r\n    }\r\n  },\r\n});\r\n\r\n// Handle layout grid and list\r\nfunction handleLayout() {\r\n  const layoutEls = container.querySelectorAll(\".product-layout-js\");\r\n  function getCurrentlayout() {\r\n    return queryString.parse(window.location.search, true).layout || \"grid\";\r\n  }\r\n  function changeLayout() {\r\n    const currentLayout = getCurrentlayout();\r\n    layoutEls.forEach((layoutEl) => {\r\n      const activeClasses = layoutEl\r\n        .getAttribute(\"data-active-class\")\r\n        .replace(/\\s+/g, \" \")\r\n        .trim()\r\n        .split(\" \");\r\n      if (currentLayout === layoutEl.getAttribute(\"data-layout\")) {\r\n        layoutEl.classList.add(...activeClasses);\r\n      } else {\r\n        layoutEl.classList.remove(...activeClasses);\r\n      }\r\n    });\r\n  }\r\n  changeLayout();\r\n\r\n  if (builderMode) {\r\n    return;\r\n  }\r\n  layoutEls.forEach((layoutEl) => {\r\n    layoutEl.addEventListener(\"click\", async () => {\r\n      const currentLayout = getCurrentlayout();\r\n      const layout = layoutEl.getAttribute(\"data-layout\");\r\n      if (layout === currentLayout) {\r\n        return;\r\n      }\r\n      const url = new URL(window.location.href);\r\n      const params = queryString.parse(url.search);\r\n      const newParams = [\r\n        ...params.filter((item) => item[0] !== \"layout\"),\r\n        [\"layout\", layout],\r\n      ];\r\n      const search = queryString.stringify(newParams);\r\n      url.search = search;\r\n      window.history.pushState(null, null, url.href);\r\n      const contentCollectionsEl = container.querySelector(\r\n        \".content-collections-js\"\r\n      );\r\n      contentCollectionsEl.style.opacity = 0.6;\r\n      const content = await getContentCollections(url);\r\n      contentCollectionsEl.style.opacity = 1;\r\n      contentCollectionsEl.innerHTML = content;\r\n      changeLayout();\r\n    });\r\n  });\r\n}\r\nhandleLayout();\r\n\r\n","liquid":"{% liquid\r\n  if section_settings.background_fixed assign backgroundFixed = 'fixed' else assign backgroundFixed = 'scroll' endif\r\n  if section_settings.background_type == 'color' assign backgroundColor = 'background-color: ' | append: section_settings.background_color | append: ';' else assign backgroundColor = '' endif\r\n  if section_settings.background_type == 'image' assign backgroundImage = 'background-image: url(' | append: section_settings.background_image | append:\r\n  ');background-attachment: ' | append: backgroundFixed | append: ';background-size: ' | append: section_settings.background_size | append: ';' else assign backgroundImage = '' endif\r\n  assign sectionPadding = 'padding-top: ' | append: section_settings.padding.top | append: 'px;padding-right: ' | append: section_settings.padding.right | append: 'px;padding-bottom: ' | append: section_settings.padding.bottom | append: 'px;padding-left: ' | append: section_settings.padding.left | append: 'px;'\r\n%}\r\n\r\n<section data-id=\"{{uniqueId}}\" css=\"{{ backgroundColor }}{{ backgroundImage }}{{ sectionPadding }}\">\r\n  {% if section_settings.background_overlay_enable %}\r\n    <div class=\"pos:absolute ins:0 z:-1\" css=\"background-color: {{ section_settings.background_overlay_color }}\"></div>\r\n  {% endif %}\r\n  <shopify>\r\n    <div data-shopify-id=\"{{ section.id }}\"></div>\r\n    {% if builderMode %}\r\n      {% assign searchParams = testing.layout %}\r\n    {% else %}\r\n      {% capture searchParams %}\r\n        {% assign pageUrlWithHost = content_for_header | split:'\"pageurl\":\"' | last | split:'\"' | first |\r\n          replace:'\\/','/' |\r\n          replace:'%20',' ' |\r\n          replace:'\\u0026','&'\r\n        %}\r\n        {{ pageUrlWithHost | split: \"?\" | last }}\r\n      {% endcapture %}\r\n    {% endif %}\r\n  </shopify>\r\n  <div class=\"container pt:35px\">\r\n    <shopify>\r\n      {% unless request.page_type == 'index' or request.page_type == 'list-collections' %}\r\n        <nav role=\"navigation\" aria-label=\"breadcrumbs\">\r\n            <a class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" href=\"{{ routes.root_url }}\" title=\"Home\">Home</a>\r\n            {% if request.page_type contains 'page' %}\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:color-gray9\">{{ page.title }}</span>\r\n            {% elsif request.page_type contains 'product' %}\r\n              {% if collection.url %}\r\n                <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n                {{ collection.title | link_to: collection.url }}\r\n              {% endif %}\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:color-gray9\">{{ product.title }}</span>\r\n            {% elsif request.page_type contains 'collection' and collection.handle %}\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n              {% if current_tags %}\r\n                {% capture url %}/collections/{{ collection.handle }}{% endcapture %}\r\n                {{ collection.title | link_to: url }}\r\n                <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n                <span class=\"fz:15px fw:400 ff:font-primary c:color-gray9\">{{ current_tags | join: \" + \" }}</span>\r\n              {% else %}\r\n                <span class=\"fz:15px fw:400 ff:font-primary c:color-gray9\">{{ collection.title }}</span>\r\n              {% endif %}\r\n            {% elsif request.page_type == 'blog' %}\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n              {% if current_tags %}\r\n                {{ blog.title | link_to: blog.url }}\r\n                <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n                <span class=\"fz:15px fw:400 ff:font-primary c:color-gray9\">{{ current_tags | join: \" + \" }}</span>\r\n              {% else %}\r\n                <span class=\"fz:15px fw:400 ff:font-primary c:color-gray9\">{{ blog.title }}</span>\r\n              {% endif %}\r\n            {% elsif request.page_type == 'article' %}\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n              {{ blog.title | link_to: blog.url }}\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n              <span>{{ article.title }}</span>\r\n            {% elsif request.page_type == 'cart' %}\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:color-gray9\">Cart</span>\r\n            {% else %}\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:#9E9ECC\" aria-hidden=\"true\">></span>\r\n              <span class=\"fz:15px fw:400 ff:font-primary c:color-gray9\">{{ page_title }}</span>\r\n            {% endif %}\r\n        </nav>\r\n      {% endunless %}\r\n    </shopify>\r\n    <div class=\"view-as mb:20px d:flex ai:center jc:space-between mt:30px fz:15px c:color-gray9 flw:wrap w:75% pr:35px@md\">\r\n      <div class=\"d:flex\">\r\n        <span class=\"product-layout-js p:0px_5px_0px_5px ml:10px cur:pointer c:color-gray2 bd:1px_solid_color-gray2\" data-layout=\"grid\" data-active-class=\"c:color-gray9! bd:1px_solid_color-gray9\">\r\n          <i class=\"fas fa-th\"></i>\r\n        </span>\r\n        <span class=\"product-layout-js p:0px_5px_0px_5px ml:10px cur:pointer c:color-gray2 bd:1px_solid_color-gray2\" data-layout=\"list\" data-active-class=\"c:color-gray9! bd:1px_solid_color-gray9\">\r\n          <i class=\"fal fa-list\"></i>\r\n        </span>\r\n      </div>\r\n      <shopify>\r\n        {% assign collection_prs = collection.products %}\r\n        {%- assign arr_price = collection_prs | map:'price_min' | uniq | sort -%}\r\n        {%- assign min_price = arr_price | first -%}\r\n        {%- assign max_price = arr_price | last -%}\r\n\r\n        {% assign my_collection_sort = collection.sort_options %}\r\n        <form id=\"filter-form\" class=\"filter_form\">\r\n          <div class=\"d:flex whs:nowrap ai:center pos:relative mt:10px mt:0px@md filter_field sort_by pos:relative\">\r\n            <p class=\"mr:10px fz:15px fw:500\">Sort by</p>\r\n            <div class=\"veda-select filter-sort-by-js\">\r\n              <div class=\"veda-select__view w:pfs(130px,190px) h:43px bd:1px_solid_#DEDEE9 pl:15px cur:pointer d:flex ai:center ff:font-secondary\">\r\n                <div class=\"veda-select__label w:pfs(100px,150px) cur:pointer d:flex ai:center fz:pfs(12px,15px) fw:400 c:color-gray4 ff:font-secondary\"></div>\r\n                <i class=\"fal fa-angle-down\"></i>\r\n              </div>\r\n              <div class=\"veda-select__options select-custom pos:absolute t:50px w:pfs(130px,190px) z:100 bgc:color-light bd:1px_solid_#DEDEE9\">\r\n                {% assign first_sort_option = my_collection_sort | first %}\r\n                {% for option in my_collection_sort %}\r\n                  <div class=\"veda-select__option select-custom__item w:pfs(128px,188px) h:43px fz:pfs(12px,15px) fw:400 pl:15px cur:pointer bgc:#DEDEE9|h! d:flex ai:center bd:none! ff:font-secondary\" value=\"{{ option.value | escape }}\" {% if first_sort_option.name  == option.name %}selected=\"true\"{% endif %} >{{ option.name | escape }}</div>\r\n                {% endfor %}\r\n\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </form>\r\n      </shopify>\r\n    </div>\r\n    <div class=\"row\">\r\n      <shopify>\r\n        <div class=\"col-lg-9 pr:40px@md d:block\">\r\n          {% if searchParams contains \"layout=list\" %}\r\n            {% assign quantity_product_displayed = 4 %}\r\n          {% else %}\r\n            {% assign quantity_product_displayed = 9 %}\r\n          {% endif %}\r\n          <div component=\"content\" class=\"content-collections-js pb:100px\">\r\n            {% paginate collection.products by quantity_product_displayed %}\r\n              {% if searchParams contains \"layout=list\" %}\r\n                {%- comment -%} wrap for list {%- endcomment -%}\r\n                <div>\r\n              {% else %}\r\n                {%- comment -%} wrap for grid {%- endcomment -%}\r\n                <div class=\"row row-cols-lg-{{ product_settings.column.lg }} row-cols-md-{{ product_settings.column.md }} row-cols-sm-{{ product_settings.column.sm }} row-cols-{{ product_settings.column.xs }}\">\r\n              {% endif %}\r\n                {% for product in collection.products %}\r\n                  {% if searchParams contains \"layout=list\" %}\r\n                    {%- comment -%} list item {%- endcomment -%}\r\n                    <div class=\"product-card-js d:flex fld:row ai:center ta:center mb:15px pb:15px fxw:wrap\">\r\n                      <script class=\"product-card-data-js\" type=\"application/json\">\r\n                        {\r\n                          id: {{ product.id | json }},\r\n                          title: {{ product.title | json }},\r\n                          vendor: {{ product.vendor | json }},\r\n                          handle: {{ product.handle | json }},\r\n                          available: {{ product.available | json }},\r\n                          content: {{ product.content | json }},\r\n                          description: {{ product.description | json }},\r\n                          featured_image: {{ product.featured_image | json }},\r\n                          images: {{ product.images | json }},\r\n                          options: {{ product.options | json }},\r\n                          price_max: {{ product.price_max | json }},\r\n                          price_min: {{ product.price_min | json }},\r\n                          type: {{ product.type | json }},\r\n                          tags: {{ product.tags | json }},\r\n                          variants: {{ product.variants | json }},\r\n                          price: {{ product.price | json }},\r\n                          options_with_values: {{ product.options_with_values | json }},\r\n                          url: {{ product.url | json }}\r\n                        }\r\n                      </script>\r\n                      <div class=\"group* w:250px pos:relative ov:hidden mr:0px mr:30px@sm\">\r\n                        <div class=\"ov:hidden w:100%\">\r\n                          <a href=\"{{product.url}}\" class=\"veda-image-cover d:block w:100%\" style=\"--aspect-ratio:{{product_settings.aspect_ratio}}\">\r\n                            <img class=\"product-card-image-js\" src=\"{{ product.featured_image.src | img_url: '250x' }}\" alt=\"{{ product.title }}\">\r\n                          </a>\r\n                        </div>\r\n\r\n                        <div class=\"pos:absolute t:5px l:5px d:flex\">\r\n                          {% if product.compare_at_price > product.price and product_settings.sale_enable %}\r\n                            <div class=\"d:flex jc:center ai:center w:60px h:30px bgc:#219653 mr:5px bdrs:2px\">\r\n                              <p class=\"fz:16px pos:absolute z:10 fw:600 c:color-light-freeze\" css=\"{{ product_settings.sale_design }}\">{{ product_settings.sale_text}}</p>\r\n                            </div>\r\n                          {% endif %}\r\n                        </div>\r\n                        <div class=\"pos:absolute t:10px r:10px w:35px h:127px\">\r\n                          {% if wishlist_action.enabled %}\r\n                              <div class=\"wishlist-toggle-js pos:relative d:flex ai:center jc:center w:36px h:36px bdrs:50% bgc:color-light fz:17px c:color-gray9 mb:10px