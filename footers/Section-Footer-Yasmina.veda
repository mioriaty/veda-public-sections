r\n            <img class=\"\" src=\"{{logo.logo_image}}\" alt=\"\">\r\n          </a>\r\n          <div class=\"ml:20px\">\r\n            <div class=\"ff:font-tertiary fw:700 fz:pfs(20px,35px) lh:46px c:color-light-freeze mb:15px\">{{logo.logo_text}}</div>\r\n            <ul class=\"fw:700 pb:25px pl:0 list-style:none pb:20px\">\r\n              <li class=\"fz:pfs(12px,15px) mb:11px c:color-light\">\r\n                <a href=\"tel:{{shop.phone}}\" class=\"op:0.7 c:color-light-freeze fw:400\">Call Us: {{shop.phone}}</a>\r\n              </li>\r\n              <li class=\"fz:pfs(12px,15px) mb:11px c:color-light\">\r\n                <a href=\"mailto:{{shop.email}}\" class=\"op:0.7 c:color-light-freeze fw:400\">{{shop.email}}</a>\r\n              </li>\r\n              <li class=\"fz:pfs(12px,15px) mb:11px c:color-light-freeze\">\r\n                <a class=\"op:0.7 c:color-light-freeze fw:400\">Monday till Friday 10 to 6 EST</a>\r\n              </li>\r\n            </ul>\r\n          </div>\r\n\r\n        </div>\r\n        {% for item in navigation %}\r\n          <div class=\"col d:flex jc:center\" component=\"navigation\">\r\n            <div class=\"w:fit-content\">\r\n              <h3 class=\"c:color-light-freeze fz:14px mb:23px fw:700 tt:uppercase ff:font-quinary w:fit-content\"\r\n              >{{item.title}}</h3>\r\n              <ul class=\"fw:700 pb:25px pl:0 list-style:none w:fit-content\">\r\n                {% for item in item.links %}\r\n                  <li class=\"fz:pfs(12px,15px) mb:9px c:color-light-freeze\">\r\n                    <a class=\"op:0.7 c:color-light-freeze fw:400\" href={{item.href}}>{{item.label}}</a>\r\n                  </li>\r\n                {% endfor %}\r\n              </ul>\r\n            </div>\r\n          </div>\r\n        {% endfor %}\r\n        {% if new_letter.enable %}\r\n          <div class=\"col\" component=\"navigation\">\r\n            <div class=\"w:fit-content\">\r\n              <h3 class=\"c:color-light-freeze fz:14px mb:23px fw:700 ff:font-quinary tt:uppercase w:fit-content\">{{new_letter.title}}</h3>\r\n              <ul class=\"fw:400 pl:0 list-style:none w:fit-content\">\r\n                <li class=\"fz:pfs(12px,15px) pb:20px c:color-light-freeze\"><a class=\"nvd-footer-list__link op:0.7 c:color-light-freeze fw:400\" href=\"#\">{{new_letter.description}}</a></li>\r\n              </ul>\r\n              {% form 'customer' %}\r\n                <div class=\"maw:309px m:auto pos:relative w:fit-content\">\r\n                  {% if form.errors %}\r\n                    {% for field in form.errors %}\r\n                      <div>\r\n                        <p>{{ field }} {{ form.errors.messages[field] }}</p>\r\n                      </div>\r\n                    {% endfor %}\r\n                  {% endif %}\r\n                  {% if form.posted_successfully? %}\r\n                    <div>\r\n                      <p>{{new_letter.success_message}}</p>\r\n                    </div>\r\n                  {% else %}\r\n                    <div class=\"d:flex w:fit-content h:40px c:color-dark bd:none bgc:white\">\r\n                      <input type=\"hidden\" name=\"contact[tags]\" value=\"newsletter\" />\r\n                      <input\r\n                        class=\"h:38px bd:none! o:none bgc:white fz:12px c:color-gray4 fz:12px||placeholder footer-input\"\r\n                        type=\"email\"\r\n                        name=\"contact[email]\"\r\n                        autocorrect=\"off\"\r\n                        autocapitalize=\"off\"\r\n                        placeholder=\"{{new_letter.placeholder}}\"\r\n                      />\r\n                      <button\r\n                        class=\"d:flex ai:center jc:center w:93px h:40px fz:12px fw:600 bgc:#FB9A00 bgc:#FB9A00|h! c:color-light cur:pointer pos:relative fz:12px fw:600 bd:1px_solid_#FB9A00 bd:1px_solid_#FB9A00|h! bdrs:0px ff:font-quinary!\"\r\n                        type=\"submit\"\r\n                        aria-label=\"submit\"\r\n                      >\r\n                        {{new_letter.button_text}}\r\n                      </button>\r\n                    </div>\r\n                    <p class=\"result\"></p>\r\n                    <p class=\"result-error\"></p>\r\n                  {% endif %}\r\n                </div>\r\n              {% endform %}\r\n              <div class=\"h:25px d:flex ai:center jc:center mt:20px fxw:wrap\">\r\n                {% for item in new_letter.icons %}\r\n                  <a href=\"{{item.link}}\">\r\n                    <span class=\"ml:10px mr:10px cur:pointer fz:18px c:#030C23 c:color-light-freeze|h!\">\r\n                      {{item.icon}}\r\n                    </span>\r\n                  </a>\r\n                {% endfor %}\r\n              </div>\r\n            </div>\r\n          </div>\r\n        {% endif %}\r\n      </div>\r\n\r\n\r\n\r\n      <div class=\"col-lg-12\">\r\n        <p class=\"ff:font-secondary ta:center op:0.7 fz:12px\">Â© 2021 BuildApp | All Rights Reserved</p>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</footer>\r\n","schema":{"blocks":[{"id":"id_8af00005-549c-432a-805a-d53befeb1962","children":[{"children":false,"type":"switch","label":"Background Fixed","summary":"","name":"background_fixed","id":"id_fb15efbf-53d2-49a2-b12e-c35c36d1011a","disable":false},{"children":"none","type":"select","label":"Background Type","summary":"","name":"background_type","id":"id_61636e6b-e857-4c7c-8a54-7e9d539e96aa","disable":false,"options":[{"label":"Image","value":"image","id":"4c8f1172-cc1b-4ad4-9980-4a88aef037a3"},{"label":"Color","value":"color","id":"32f1bf56-ef03-465e-9202-2d480cef901d"},{"label":"None","value":"none","id":"6e895dd2-848e-45a3-8c75-ddacbb0208c7"}]},{"children":"","type":"image","label":"Background Image","summary":"","name":"background_image","id":"id_53158124-3596-4f00-b355-ba2333968246","disable":false,"deps":"section_settings.background_type === \"image\""},{"children"